#include<iostream>
#include<vector>
using namespace std;
int main(){
    vector<int>num;
    int n;
    cout<<"enter the number how many time you want to enter the number"<<endl;
    cin>>n;

 cout<<"enter the number "<<n<<endl;

for(int i=0;i<n;i++){
   int a;
   cin>>a;
   num.push_back(a);
}
cout<<"print the number you have enter "<<endl;
for(int i=0;i<num.size();i++){
    cout<<num[i];
}cout<<endl;
return 0;
}
    
